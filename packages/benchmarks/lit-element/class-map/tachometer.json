{
  "$schema": "https://raw.githubusercontent.com/Polymer/tachometer/master/config.schema.json",
  "sampleSize": 50,
  "timeout": 10,
  "horizons": ["0%"],
  "benchmarks": [
    {
      "url": "lit-element/class-map/index.html",
      "browser": {
        "name": "chrome",
        "headless": true
      },
      "measurement": [
        {
          "mode": "performance",
          "entryName": "render"
        },
        {
          "mode": "performance",
          "entryName": "update"
        },
        {
          "mode": "performance",
          "entryName": "nop-update"
        },
        {
          "mode": "performance",
          "entryName": "update-and-nop"
        }
      ],
      "expand": [
        {
          "name": "this-change"
        }
      ]
    },
    {
      "url": "lit-element/class-map/index.html?classVariant=class-string-interpolation",
      "browser": {
        "name": "chrome",
        "headless": true
      },
      "measurement": [
        {
          "mode": "performance",
          "entryName": "render",
          "name": "render-string"
        },
        {
          "mode": "performance",
          "entryName": "update",
          "name": "update-string"
        },
        {
          "mode": "performance",
          "entryName": "nop-update",
          "name": "nop-update-string"
        },
        {
          "mode": "performance",
          "entryName": "update-and-nop",
          "name": "update-and-nop-string"
        }
      ],
      "expand": [
        {
          "name": "this-change"
        }
      ]
    }
  ]
}
