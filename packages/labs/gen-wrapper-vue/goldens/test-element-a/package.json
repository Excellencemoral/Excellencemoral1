    "ElementEvents.*",
    "ElementProps.*",
    "ElementSlots.*"
  ],
  "dependencies": {
    "@lit-internal/test-element-a": "^1.0.0",
    "@lit-labs/vue-utils": "^0.1.0",
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^9.0.1",
    "@vitejs/plugin-vue": "^3.1.2",
    "typescript": "~4.7.4",
    "vite": "^3.1.8",
    "vue-tsc": "^1.0.8"
  }
}
