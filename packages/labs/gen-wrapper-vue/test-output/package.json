{
  "name": "@lit-internal/gen-wrapper-vue-test-output",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@esm-bundle/chai": "^4.1.5",
    "@types/chai": "^4.0.1",
    "@types/mocha": "^9.0.0",
    "vue": "^3.2.25",
    "@lit-labs/vue-utils": "../../vue-utils",
    "@lit-internal/test-element-a": "file:../../test-projects/test-element-a/lit-internal-test-element-a-1.0.0.tgz",
    "@lit-internal/test-element-a-vue": "file:../gen-output/test-element-a-vue/lit-internal-test-element-a-vue-1.0.0.tgz",
    "@lit-internal/test-element-events": "file:../../test-projects/test-element-events/lit-internal-test-element-events-1.0.0.tgz",
    "@lit-internal/test-element-events-vue": "file:../gen-output/test-element-events-vue/lit-internal-test-element-events-vue-1.0.0.tgz",
    "@lit-internal/test-element-slots": "file:../../test-projects/test-element-slots/lit-internal-test-element-slots-1.0.0.tgz",
    "@lit-internal/test-element-slots-vue": "file:../gen-output/test-element-slots-vue/lit-internal-test-element-slots-vue-1.0.0.tgz",
    "@lit-internal/test-element-props": "file:../../test-projects/test-element-props/lit-internal-test-element-props-1.0.0.tgz",
    "@lit-internal/test-element-props-vue": "file:../gen-output/test-element-props-vue/lit-internal-test-element-props-vue-1.0.0.tgz"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.1",
    "typescript": "~4.7.4",
    "vite": "^2.9.2",
    "vue-tsc": "^0.29.8"
  },
  "scripts": {
    "installSelf": "wireit",
    "build": "wireit",
    "test": "wireit",
    "build:self:watch": "vue-tsc --noEmit && vite build --watch",
    "test:self:watch": "vue-tsc --noEmit && vite build && node ../../../tests/run-web-tests.js \"tests/tests.js\" --config ../../../tests/web-test-runner.config.js --watch"
  },
  "wireit": {
    "installSelf": {
      "dependencies": [
        "../:test:gen"
      ],
      "files": [
        "package.json"
      ],
      "command": "npm i",
      "output": [
        "node_modules",
        "package-lock.json"
      ]
    },
    "build": {
      "dependencies": [
        "installSelf"
      ],
      "files": [
        "src/**/*.ts",
        "src/**/*.vue",
        "tsconfig.json"
      ],
      "command": "vue-tsc --noEmit && vite build",
      "output": [
        "tests",
        "tsconfig.tsbuildinfo"
      ]
    },
    "test": {
      "dependencies": [
        "build",
        "../../../tests:build"
      ],
      "files": [],
      "command": "node ../../../tests/run-web-tests.js \"tests/tests.js\" --config ../../../tests/web-test-runner.config.js",
      "output": []
    }
  }
}
