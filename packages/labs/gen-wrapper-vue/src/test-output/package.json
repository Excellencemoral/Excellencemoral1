{
  "name": "@lit-internal/gen-wrapper-vue-test-output",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@esm-bundle/chai": "^4.1.5",
    "@types/chai": "^4.0.1",
    "@types/mocha": "^9.0.0",
    "vue": "^3.2.25",
    "@lit-internal/test-element-a": "file:../../test-projects/test-element-a/lit-internal-test-element-a-1.0.0.tgz",
    "@lit-internal/test-element-a-vue": "file:../gen-output/test-element-a-react/lit-internal-test-element-a-vue-1.0.0.tgz"
  },
  "scripts": {
    "installSelf": "wireit",
    "build": "wireit",
    "build:ts": "wireit",
    "test": "wireit"
  },
  "wireit": {
    "installSelf": {
      "dependencies": [
        "../:test:gen"
      ],
      "files": [
        "package.json"
      ],
      "command": "npm i",
      "output": [
        "node_modules",
        "package-lock.json"
      ]
    },
    "build:ts": {
      "dependencies": [
        "installSelf"
      ],
      "files": [
        "src/**/*.ts",
        "src/**/*.tsx",
        "tsconfig.json"
      ],
      "command": "tsc --build --pretty",
      "output": [
        "js"
      ]
    },
    "build": {
      "dependencies": [
        "build:ts"
      ],
      "files": [
        "rollup.config.js"
      ],
      "command": "rollup -c",
      "output": [
        "*.{js,js.map,d.ts,d.ts.map}",
        "!rollup.config.js"
      ]
    },
    "test": {
      "dependencies": [
        "build"
      ],
      "files": [
        "../../../tests/run-web-tests.js",
        "../../../tests/web-test-runner.config.js",
        "../../../tests/wtr-config.js",
        "../../../tests/rollup-resolve-remap.js"
      ],
      "command": "node ../../../tests/run-web-tests.js \"tests/**/*_test.js\" --config ../../../tests/web-test-runner.config.js",
      "output": []
    }
  }
}
