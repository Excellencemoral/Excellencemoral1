{
  "name": "@lit-labs/slottable-request",
  "version": "0.1.0",
  "description": "Lit helpers for working with the Slottable Request protocol",
  "license": "BSD-3-Clause",
  "homepage": "https://lit.dev/",
  "repository": {
    "type": "git",
    "url": "https://github.com/lit/lit.git",
    "directory": "packages/labs/slottable-request"
  },
  "type": "module",
  "main": "slottable-request-event.js",
  "module": "slottable-request-event.js",
  "typings": "slottable-request-event.d.ts",
  "directories": {
    "test": "test"
  },
  "exports": {
    ".": {
      "types": "./development/slottable-request-event.d.ts",
      "development": "./development/slottable-request-event.js",
      "default": "./slottable-request-event.js"
    },
    "./slottable-request-event.js": {
      "types": "./development/slottable-request-event.d.ts",
      "development": "./development/slottable-request-event.js",
      "default": "./slottable-request-event.js"
    },
    "./render-slottable.js": {
      "types": "./development/render-slottable.d.ts",
      "development": "./development/render-slottable.js",
      "default": "./render-slottable.js"
    },
    "./request-slottable.js": {
      "types": "./development/request-slottable.d.ts",
      "development": "./development/request-slottable.js",
      "default": "./request-slottable.js"
    }
  },
  "files": [
    "/development/",
    "!/development/test/",
    "/slottable-request-event.{d.ts,d.ts.map,js,js.map}",
    "/render-slottable.{d.ts,d.ts.map,js,js.map}",
    "/request-slottable.{d.ts,d.ts.map,js,js.map}"
  ],
  "scripts": {
    "build": "wireit",
    "build:ts": "wireit",
    "build:ts:types": "wireit",
    "build:rollup": "wireit",
    "test": "wireit",
    "test:dev": "wireit",
    "test:prod": "wireit",
    "checksize": "wireit"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "build:ts",
        "build:ts:types",
        "build:rollup",
        "../../lit:build"
      ]
    },
    "build:ts": {
      "command": "tsc --build --pretty",
      "dependencies": [
        "../../reactive-element:build:ts:types",
        "../../lit:build:ts:types"
      ],
      "clean": "if-file-deleted",
      "files": [
        "src/**/*.ts",
        "tsconfig.json"
      ],
      "output": [
        "development",
        "tsconfig.tsbuildinfo"
      ]
    },
    "build:ts:types": {
      "command": "treemirror development . \"**/*.d.ts{,.map}\"",
      "dependencies": [
        "../../internal-scripts:build",
        "build:ts"
      ],
      "files": [],
      "output": [
        "*.d.ts{,.map}",
        "test/**/*.d.ts{,.map}"
      ]
    },
    "build:rollup": {
      "command": "rollup -c",
      "dependencies": [
        "build:ts"
      ],
      "files": [
        "rollup.config.js",
        "../../../rollup-common.js"
      ],
      "output": [
        "slottable-request-event.js{,.map}",
        "render-slottable.js{,.map}",
        "request-slottable.js{,.map}",
        "test/**/*.js{,.map}"
      ]
    },
    "checksize": {
      "command": "rollup -c --environment=CHECKSIZE",
      "dependencies": [
        "build:ts"
      ],
      "files": [
        "rollup.config.js",
        "../../../rollup-common.js"
      ],
      "output": []
    },
    "test": {
      "dependencies": [
        "test:dev",
        "test:prod"
      ]
    },
    "test:dev": {
      "command": "MODE=dev node ../../tests/run-web-tests.js \"development/**/*_test.js\" --config ../../tests/web-test-runner.config.js",
      "dependencies": [
        "build",
        "../../tests:build"
      ],
      "env": {
        "BROWSERS": {
          "external": true
        }
      },
      "files": [],
      "output": []
    },
    "test:prod": {
      "command": "MODE=prod node ../../tests/run-web-tests.js \"development/**/*_test.js\" --config ../../tests/web-test-runner.config.js",
      "dependencies": [
        "build",
        "../../tests:build"
      ],
      "env": {
        "BROWSERS": {
          "external": true
        }
      },
      "files": [],
      "output": []
    }
  },
  "author": "Google LLC",
  "dependencies": {
    "lit": "^2.0.0"
  },
  "devDependencies": {
    "@types/trusted-types": "^2.0.2",
    "@lit-internal/scripts": "^1.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
