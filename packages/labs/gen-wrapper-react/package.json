{
  "private": true,
  "name": "@lit-labs/gen-wrapper-react",
  "description": "Code generator for React wrapper for Lit components",
  "version": "0.0.0",
  "author": "Google LLC",
  "license": "BSD-3-Clause",
  "bugs": "https://github.com/lit/lit/issues",
  "type": "module",
  "main": "lib/index.js",
  "repository": "lit/lit",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "npm run test:gen && npm run test:output",
    "test:gen": "uvu test-gen '.*_test\\.js$'",
    "test:output": "node ../../tests/run-web-tests.js \"test-output/**/*_test.js\" --config ../../tests/web-test-runner.config.js"
  },
  "dependencies": {
    "@lit-labs/analyzer": "^0.1.1",
    "@lit-labs/gen-utils": "^0.0.0"
  },
  "devDependencies": {
    "@esm-bundle/chai": "^4.1.5",
    "@types/chai": "^4.0.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.31",
    "typescript": "~4.6.2",
    "uvu": "^0.5.3"
  },
  "engines": {
    "node": ">=14.8.0"
  },
  "files": [
    "index.js"
  ],
  "homepage": "https://github.com/lit/lit",
  "keywords": [
    "lit",
    "lit-html",
    "lit-element",
    "LitElement",
    "generator",
    "cli",
    "react"
  ]
